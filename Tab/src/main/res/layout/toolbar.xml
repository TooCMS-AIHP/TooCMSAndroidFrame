<?xml version="1.0" encoding="utf-8"?>
<com.toocms.tab.control.TooCMSToolbar xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/toolbar"
    android:layout_width="match_parent"
    android:layout_height="@dimen/action_height_size"
    android:background="@color/action_bg"
    android:gravity="center_vertical"
    android:minHeight="@dimen/action_height_size"
    app:navigationIcon="@drawable/ic_menu_back">

    <TextView
        android:id="@+id/title"
        style="@style/TitleTextAppearance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</com.toocms.tab.control.TooCMSToolbar>